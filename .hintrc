{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent",
          "user-select",
          "mask-image",
          "mask-position",
          "print-color-adjust",
          "-webkit-text-size-adjust",
          "mask-size",
          "text-decoration-skip-ink",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling",
          "user-select: all",
          "overflow-anchor"
        ]
      }
    ],
    "axe/parsing": "off",
    "no-inline-styles": "off",
    "css-prefix-order": "off"
  }
}